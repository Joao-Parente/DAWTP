extends layout

block content
    .w3-card-4
      header.w3-container.w3-teal
        h3.w3-left Relatorio Lista (DAW2020)
      if list.length > 0
        .w3-container 
          table.w3-table-all
            tr 
              th Titulo

            each s in list
              tr
                td=s.likes
      else
        .w3-container
          p Não há relatórios..
      footer.w3-container.w3-teal
        address Gerado por dawApp :: 2020-Nov, jcr