extends layout

block content


  .w3-container.w3-margin
    h1 Tipo de ficheiros #{tipo.nome}... 


    table.w3-table-all
      tr
        th Nome Parametro
        th Valor Parametro

      each meta in tipo.parametros
        tr
          td=meta.nome_param
          td=meta.tipo_param


    address.w3-container.w3-margin Gerado por dawApp :: 2020-Nov
      p
        a(href="/") Index
      p
        a(href="/tipos") Tipos
